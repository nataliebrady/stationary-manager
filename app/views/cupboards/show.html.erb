<% provide(:title, @cupboard.name) %>

<div class="page-container">

<h2>
  <%= @cupboard.name %><br>
<h2>

<% @cupboard.items.each do |item| %>
  <h5>Product code:</h5>
  <%= link_to item.id, item_url(item) %>

  <h5>Product name:</h5>
  <%= link_to item.name, item_url(item) %><br>
  
  <br><br>
<% end %>

</div>